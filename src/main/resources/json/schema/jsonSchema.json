{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/plan.json",
  "type": "object",
  "title": "The root schema",
  "description": "The schema for the json format of medical plan",
  "default": {},
  "required": [
    "_org",
    "objectId",
    "objectType",
    "planType",
    "creationDate"
  ],
  "properties": {
    "_org": {
      "type": "string",
      "description": "The organization of plan.",
      "examples": [
        "example.com"
      ]
    },
    "objectId": {
      "type": "string",
      "description": "The unique identifier of object.",
      "examples": [
        "12xvxc345ssdsds-508"
      ]
    },
    "objectType": {
      "type": "string",
      "description": "The type of object.",
      "examples": [
        "plan"
      ]
    },
    "planType": {
      "type": "string",
      "description": "The type of plan.",
      "examples": [
        "inNetwork"
      ]
    },
    "creationDate": {
      "type": "string",
      "description": "The creation date of plan.",
      "examples": [
        "12-12-2017"
      ]
    }
  }
}